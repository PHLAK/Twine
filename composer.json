{
    "name": "phlak/twine",
    "description": "String manipulation, leveled up!",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Kankiewicz",
            "email": "Chris@ChrisKankiewicz.com"
        }
    ],
    "support": {
        "issues": "https://github.com/PHLAK/Twine/issues"
    },
    "require": {
        "php": ">=7.0",
        "ext-openssl": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^1.0 || ^2.0",
        "phlak/chronometer": "^1.0",
        "phpunit/phpunit": "^6.0 || ^7.0",
        "psy/psysh": "^0.9.0",
        "symfony/console": "^3.0 || ^4.0"
    },
    "autoload": {
        "psr-4": {
            "PHLAK\\Twine\\": "src/"
        },
        "files": ["src/Support/helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "PHLAK\\Twine\\Benchmarks\\": "benchmarks/",
            "PHLAK\\Twine\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
